<h1> Users#index </h1>


<% if current_user %>
    <h2>
      Welcome to Instagram,
      <%= current_user.name.capitalize %>!
    </h2>
  <% end %>

<% @users.each do |usertest| %>
  <p>
  <span><%= usertest.name %></span>
  <span><%= usertest.email %></span>
  <span><%= usertest.password %></span>
  </p>
<% end %>
